@if (User.Identity.IsAuthenticated)
{
    <div class="row">
        <div class="columns small-12 large-4">
            @Html.Partial("_Avengers")
        </div>
        <div class="columns small-12 large-8">
            @Html.Partial("_Missions")
        </div>
    </div>
}
else
{
    @Html.Partial("_Login")
}